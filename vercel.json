{
    "version": 2,
    "builds": [
        {
            "src": "bOSS_django/bOSS_django/wsgi.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "bOSS_django/bOSS_django/wsgi.py"
        }
    ]
}
